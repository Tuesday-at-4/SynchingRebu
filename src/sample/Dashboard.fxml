<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="anchorDashboard" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Dashboard_Controller">
   <children>
      <HBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleDriverButton" text="Driver" />
            <Button mnemonicParsing="false" onAction="#handlePassengerButton" text="Passenger" />
            <Button mnemonicParsing="false" onAction="#handleAccountButton" text="Account" />
         </children>
      </HBox>
   </children>
</AnchorPane>
