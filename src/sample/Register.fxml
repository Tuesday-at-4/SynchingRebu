<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;"
  xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="sample.RegisterController">
  <children>
    <Label alignment="CENTER" layoutX="205.0" layoutY="41.0" prefHeight="26.0" prefWidth="190.0"
      text="CREATE AN ACCOUNT" textFill="WHITE">
      <font>
        <Font name="System Bold" size="17.0"/>
      </font>
    </Label>
    <Label layoutX="150.0" layoutY="90.0" text="FIRST NAME" textFill="WHITE">
      <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
      </padding>
    </Label>
    <Label layoutX="150.0" layoutY="130.0" text="LAST NAME" textFill="WHITE">
      <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
      </padding>
    </Label>
    <Label layoutX="150.0" layoutY="170.0" text="EMAIL" textFill="WHITE">
      <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
      </padding>
    </Label>
    <Label layoutX="150.0" layoutY="210.0" text="PHONE" textFill="WHITE">
      <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
      </padding>
    </Label>
    <Label layoutX="150.0" layoutY="250.0" text="DOB" textFill="WHITE">
      <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
      </padding>
    </Label>
    <TextField fx:id="txtField_firstName" layoutX="240.0" layoutY="90.0" prefHeight="25.0"
      prefWidth="180.0"/>
    <TextField fx:id="txtField_lastName" layoutX="240.0" layoutY="130.0" prefHeight="25.0"
      prefWidth="180.0"/>
    <TextField fx:id="txtField_email" layoutX="240.0" layoutY="170.0" prefHeight="25.0"
      prefWidth="180.0"/>
    <TextField fx:id="txtField_phone" layoutX="240.0" layoutY="210.0" prefHeight="25.0"
      prefWidth="180.0"/>
    <Button fx:id="btn_createAccount" layoutX="304.0" layoutY="365.0" mnemonicParsing="false"
      onMouseClicked="#create_account" text="CREATE ACCOUNT"/>
    <Label layoutX="150.0" layoutY="290.0" text="USERNAME" textFill="WHITE"/>
    <Label layoutX="150.0" layoutY="330.0" text="PASSWORD" textFill="WHITE"/>
    <TextField fx:id="txtField_createUsername" layoutX="240.0" layoutY="290.0" prefHeight="25.0"
      prefWidth="180.0"/>
    <DatePicker fx:id="dateB_DOB" layoutX="240.0" layoutY="250.0" prefHeight="26.0"
      prefWidth="180.0"/>
    <PasswordField fx:id="txtField_createPassword" layoutX="240.0" layoutY="330.0" prefHeight="25.0"
      prefWidth="180.0"/>
  </children>
  <padding>
    <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
  </padding>
</AnchorPane>
